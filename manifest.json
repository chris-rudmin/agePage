{
  "name": "agePage",
  "version": "0.1",
  "manifest_version": 2,
  "description": "An extension which adds visual cues to inform the user how old the page is.",
  "author": "Chris Rudmin",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://www.google.com/*"
  ],
  "content_scripts": [{
    "matches": [ "http://*/*", "https://*/*" ],
    "js": [ "src/jquery-2.0.3.min.js", "src/agePage.js" ],
    "css": [ "src/agePage.css" ]
  }],
  "web_accessible_resources": [ "images/yellow.jpg" ]
}